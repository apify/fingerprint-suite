    "version": "2.0.0",
    "description": "",
    "workspaces": [
        "packages/*"
    ],
    "keywords": [
        "apify",
        "headless",
        "chrome",
        "puppeteer",
        "crawler",
        "scraper"
    ],
    "author": {
        "name": "Apify",
        "email": "support@apify.com",
