    "version": "2.1.70",
    "description": "",
    "workspaces": [
        "packages/*"
    ],
    "keywords": [
        "apify",
        "headless",
        "chrome",
        "puppeteer",
        "crawler",
        "scraper"
    ],
    "author": {
        "name": "Apify",
        "email": "support@apify.com",
