        "semver": "^7.3.8",
        "ts-node": "^10.8.0",
        "turbo": "^2.4.4",
        "typescript": "^5.0.0",
        "typescript-eslint": "^8.28.0",
        "vite": "^7.1.3",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.2.4"
    },
    "overrides": {
        "eslint-config-airbnb-base": {
            "eslint": "$eslint"
        }
    },
    "packageManager": "npm@11.5.2"
