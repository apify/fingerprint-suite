{
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "copy": {
            "dependsOn": [
                "^copy"
            ],
            "outputs": [
                "dist/**"
            ],
            "env": ["GIT_TAG"]
        },
        "clean": {
            "outputs": []
        }
    }
}
